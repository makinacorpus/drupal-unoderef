{
    "compilerOptions": {
        "allowJs": true,
        "alwaysStrict": true,
        "checkJs": false,
        "downlevelIteration": false,
        "inlineSources": true,
        "mapRoot": "./dist/",
        "module": "system",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "outDir": "./dist/",
        "outFile": "./dist/unoderef.js",
        "removeComments": true,
        "rootDir": "./js/",
        "sourceMap": true,
        "sourceRoot": "./dist/",
        "strict": true,
        "strictNullChecks": true,
        "target": "es5",
        "typeRoots": [
            "./node_modules/@types"
        ]
    },
    "files": [
        "js/behavior.ts",
        "js/unoderef.ts"
    ]
}
